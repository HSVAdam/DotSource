{
    "diffEditor.codeLens": true,
    "editor.codeLens": false,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.inlineSuggest.enabled": false,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.showFoldingControls": "always",
    "editor.suggest.showFiles": false,
    "editor.suggest.showFolders": false,
    "editor.suggest.showSnippets": false,
    "editor.snippetSuggestions": "none",
    "editor.tabCompletion": "on",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "explorer.confirmDelete": false,
    "extensions.autoUpdate": true,
    "files.defaultLanguage": "powershell",
    "files.enableTrash": false,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "powershell.codeFolding.showLastLine": false,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.ignoreOneLineBlock": true,
    "powershell.codeFormatting.newLineAfterOpenBrace": true,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationAfterEveryPipeline",
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.integratedConsole.focusConsoleOnExecute": true,
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.powerShellAdditionalExePaths": {
        "PS5.1": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -nologo",
        "PS7": "C:\\Program Files\\PowerShell\\7\\pwsh.exe -nologo"
    },
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x64)",
    "powershell.startAutomatically": true,
    "redhat.telemetry.enabled": false,
    "sortJSON.contextMenu": {
        "sortJSONAlphaNum": false,
        "sortJSONAlphaNumReverse": false,
        "sortJSONKeyLength": false,
        "sortJSONKeyLengthReverse": false,
        "sortJSONReverse": false,
        "sortJSONType": false,
        "sortJSONTypeReverse": false,
        "sortJSONValues": false,
        "sortJSONValuesReverse": false
    },
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.profiles.windows": {
        "PowerShell 5": {
            "args": [
                "-nologo"
            ],
            "icon": "terminal-powershell",
            "path": [
                "${env:windir}\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
            ]
        },
        "PowerShell Core": {
            "args": [
                "-nologo"
            ],
            "icon": "terminal-powershell",
            "source": "PowerShell"
        }
    },
    "update.showReleaseNotes": false,
    "vsicons.dontShowNewVersionMessage": true,
    "vsicons.presets.foldersAllDefaultIcon": true,
    "vsicons.projectDetection.disableDetect": true,
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "todo-tree.general.enableFileWatcher": true,
    "terminal.integrated.fontFamily": "Hack NF",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "editor.accessibilitySupport": "off",
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true
}